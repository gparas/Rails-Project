<% tabsNumber = 1..5 %>
<% tabsStyle = 'mobiscrolldd' %>
<% listStyle = 'mobiscrollll' %>

<div class="container">
	<div class="row">
		<div id="sidebar" class="col-md-3" role="complementary">
			<%= render 'results/filter', listStyle: listStyle %>
		</div>
		<div id="main" class="col-md-9" role="main">
			<%= render 'results/tabs', tabsNumber: tabsNumber, tabsStyle: tabsStyle %>
			<%= render 'results/tabs_content', tabsNumber: tabsNumber, tabsStyle: tabsStyle %>
		</div>
	</div>
</div>